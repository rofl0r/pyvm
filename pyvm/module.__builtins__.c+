	/* Exception hierarchy */
#define mkexc(X,Y) DynExceptions.X = mk_stdexc (D, VSTR(#X), DynExceptions.Y);
	DynExceptions.Exception = mk_stdexc (D, VSTR ("Exception"));
	DynExceptions.Exception->setattr (Interns.__init__,
