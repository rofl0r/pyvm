	int i;

	if_unlikely (T1->len != T2->len)
		RaiseNotImplemented ("tuple_or");

	for (i = 0; i < T2->len; i++)
		if (T2->__xgetitem__ (i) != &None)
			goto doit;

	return argv [0].o;
 doit:;
	Tuplen *T = new Tuplen __sizector (T2->len);
	for (i = 0; i < T2->len; i++)
		T->__xinititem__ (i, T2->__xgetitem__ (i) != &None
				 ? T2->__xgetitem__ (i) : T1->__xgetitem__ (i));
	return T;
}

